{
    "name": "spot/cms",
    "description": "Content management system",
    "type": "Application",
    "license": "MIT",
    "authors": [
        {
            "name": "Jelmer Schreuder",
            "email": "j.schreuder@mijnpraktijk.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/WebspotCode/SpotApi"
        }
    ],
    "require": {
        "spot/api": "~1.0.0-dev",
        "zendframework/zend-diactoros": "^1.3",
        "monolog/monolog": "^1.20",
        "pimple/pimple": "^3.0",
        "nikic/fast-route": "^1.0",
        "particle/validator": "^2.2",
        "particle/filter": "^1.4",
        "robmorgan/phinx": "^0.6",
        "ramsey/uuid": "^3.4",
        "tobscure/json-api": "^0.3",
        "league/flysystem": "^1.0",
        "imagine/imagine": "^0.6",
        "symfony/console": "^3.1"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.5",
        "phpspec/prophecy": "^1.6",
        "henrikbjorn/phpspec-code-coverage": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Spot\\": "src"
        }
    }
}
