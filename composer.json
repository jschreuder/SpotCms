{
    "name": "spot/cms",
    "description": "Content management system",
    "type": "Application",
    "license": "MIT",
    "authors": [
        {
            "name": "Jelmer Schreuder",
            "email": "j.schreuder@mijnpraktijk.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/WebspotCode/SpotApi"
        }
    ],
    "require": {
        "spot/api": "~1.0.0-dev",
        "zendframework/zend-diactoros": "^1.1",
        "monolog/monolog": "^1.17",
        "pimple/pimple": "^3.0",
        "nikic/fast-route": "~0.6.0",
        "particle/validator": "^2.0",
        "particle/filter": "^1.0",
        "robmorgan/phinx": "~0.5.0",
        "ramsey/uuid": "^3.0",
        "tobscure/json-api": "~0.2.1",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "dev-master",
        "phpspec/prophecy": "dev-master",
        "henrikbjorn/phpspec-code-coverage": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Spot\\": "src"
        }
    }
}
